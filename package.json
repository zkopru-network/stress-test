{
  "name": "@zkopru/generator",
  "version": "1.0.0-beta.2",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "zkopru-wallet": "dist/wallet.js",
    "zkopru-coordinator": "dist/coordinator.js"
  },
  "keywords": [
    "generator"
  ],
  "preferGlobal": true,
  "scripts": {
    "prebuild": "shx mkdir -p dist",
    "postinstall": "yarn link-modules",
    "build": "tsc --build tsconfig.build.json",
    "clean": "tsc --build tsconfig.build.json --clean && shx rm -rf coverage *.log junit.xml dist && jest --clearCache",
    "link-modules": "link-module-alias",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters='jest-junit'",
    "coverage": "jest --coverage",
    "coverage:show": "live-server coverage",
    "dev:coordinator": "ts-node src/coordiantor.ts --config coordinator.dev.json",
    "dev:wallet": "ts-node src/wallet.ts --config wallet.dev.json"
  },
  "dependencies": {
    "async-lock": "^1.3.0",
    "axios": "^0.21.1",
    "big-integer": "^1.6.48",
    "bip39": "^3.0.2",
    "blessed": "^0.1.81",
    "bn.js": "^5.2.0",
    "bullmq": "^1.34.0",
    "chalk": "^4.0.0",
    "cli-progress": "^3.8.2",
    "figlet": "^1.4.0",
    "follow-redirects": "^1.11.0",
    "fs-extra": "^9.0.0",
    "node-ansiparser": "^2.2.0",
    "node-ansiterminal": "^0.2.1-beta",
    "node-fetch": "^2.6.1",
    "pino-pretty": "^4.5.0",
    "soltypes": "^1.3.5",
    "tar": "^6.0.2",
    "web3": "1.2.11",
    "web3-core": "1.2.11",
    "web3-eth-contract": "1.2.11",
    "web3-utils": "1.2.11",
    "yargs": "^15.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@typechain/web3-v1": "^2.2.0",
    "@types/jest": "^27.0.1",
    "jest": "^24.7.1",
    "link-module-alias": "^1.2.0",
    "shx": "^0.3.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^3.8.3"
  }
}
